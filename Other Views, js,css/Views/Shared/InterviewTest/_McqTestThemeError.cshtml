@{
    var message = ViewBag.CustomMessage as string;
    if (string.IsNullOrEmpty(message))
    {
        message = "UnExpected Error Occurred, Please Contact Help Desk Team Immeditely";
    }

}
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
<!-- Include this 'test.css' style sheet in the project where you use this test page -->
<link href="~/MvcScriptsAndStyles/css/Test/McqTest.css" rel="stylesheet" />
<section class="test-main mt-5">
    <div class="container">
        <div class="row">
            <div class="col-md-12 test-wrapper">
                <div class="thanks-message text-center" id="text-message">
                    <img src="data:image/jpeg;base64," width="100" class="mb-4">
                    <h5>@Html.Raw(message)</h5>
                </div>
            </div>
        </div>
    </div>
</section>
<script>
	$(".test-right").empty();
</script>